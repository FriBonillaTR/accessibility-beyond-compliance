	"scripts": {
		"comment1": "---------- start dev server ----------",
		"copy:build-and-css": "node ./scripts/copy-build-and-css-to-static.js",
		"serve": "serve src/static",
		"watch:rollup": "rollup -c ./rollup/rollup.config.esm.js -w",
		"start": "yarn watch:rollup & yarn copy:build-and-css & yarn serve",
		"comment2": "---------- build ----------",
		"build:rollup:esm": "cross-env NODE_OPTIONS='--no-warnings=ExperimentalWarning' rollup -c ./rollup/rollup.config.esm.js",
		"build:rollup:cjs-umd-es5": "cross-env NODE_OPTIONS='--no-warnings=ExperimentalWarning' rollup -c ./rollup/rollup.config.js",
		"build-externals": "node ./rollup/copy-bundled-deps-dist.js",
		"gen-manifest": "npx cem analyze",
		"build": "npx rimraf ./dist && yarn run build:rollup:esm && yarn build-externals && yarn run build:rollup:cjs-umd-es5 && yarn gen-manifest",
		"build:dev": "npx rimraf ./dist && yarn run build:rollup:esm && yarn build-externals && yarn gen-manifest",
		"local-pack": "yarn build:dev && yarn pack",
		"comment3": "---------- storybook ----------",
		"build-storybook:scss": "sass --style compressed --load-path=node_modules --no-source-map styles/index.scss:styles/index.css",
		"start:storybook": "yarn run build-storybook:scss && storybook dev -p 3000 -h localhost",
		"build-storybook": "yarn storybook build",
		"build:storybook": "yarn run build-storybook:scss && yarn run build-storybook",
		"comment-parser": "npx --yes tsx ./.storybook/boot-scripts/commentParser.ts",
		"comment4": "---------- other ----------",
		"test": "yarn build:storybook && NODE_NO_WARNINGS=1 npx playwright install && playwright test",
		"generate-allure-report": "npx allure generate allure-results --clean",
		"modify-base-url-allure-report": "node ./scripts/modify-base-url-allure-report.js",
		"lint:ts": "tsc --noEmit"
	},
